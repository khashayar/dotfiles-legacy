# Disguise as Chrome on OS X Yosemite
user-agent = "Mozilla/5.0 (Macintosh; Intel Mac OS X 10_10_0) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/41.0.2234.0 Safari/537.36"

# When following a redirect, automatically set the previous URL as referer
referer = ";auto"

# Wait 60 seconds before timing out
connect-timeout = 60
